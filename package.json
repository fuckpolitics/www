{
  "name": "@www/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "mg:c": "ts-node --project db/tsconfig.json ./node_modules/typeorm/cli.js migration:create db/migrations/Migration",
    "mg:g": "ts-node --project db/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate db/migrations/Migration -d db/data-source.ts",
    "mg:run": "ts-node -r ts-node/register -r tsconfig-paths/register libs/database/src/cli/run-migrations.ts",
    "mg:run:tsx": "tsx --tsconfig tsconfig.base.json libs/database/src/cli/run-migrations.ts",
    "mg:r": "ts-node -r ts-node/register -r tsconfig-paths/register libs/database/src/cli/revert-migration.ts",
    "mg:s": "ts-node -r ts-node/register -r tsconfig-paths/register libs/database/src/cli/show-migrations.ts",
    "proto:gen": "rm -rf libs/common/src/generated-grpc/* && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto -I ./libs/common/src/proto --ts_proto_out=libs/common/src/generated-grpc --ts_proto_opt=nestJs=true ./libs/common/src/proto/*.proto",
    "dev": "nx run-many -t serve"
  },
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.14",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/platform-socket.io": "^11.1.14",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^11.1.14",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^1.6.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "socket.io": "^4.8.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
